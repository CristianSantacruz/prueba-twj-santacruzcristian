<div style='background-color:white;color:black;'>

  <h1>Crear Celulares</h1>

  <form action="celular/crearCelular" method="post">
    <div class="form-group">
      <label>Nombre</label>
      <input required type="text" class="form-control"  placeholder="Nombre" name="nombre">
    </div>

    <div class="form-group">
      <label>Sistema Operativo</label>
      <input required type="text" class="form-control"  placeholder="Sistema Operativo" name="sistemaOperativo">
    </div>

    <div class="form-group">
      <label>Versión</label>
      <input required type="integer" class="form-control"  placeholder="Versión" name="version">
    </div>

    <button type="submit" class="btn btn-success">Crear Celular</button>
    <br>
  </form>
</div>
